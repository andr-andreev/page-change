{
  "name": "andr-andreev/yii2-page-change",
  "description": "Yii 2 Page Change",
  "homepage": "https://github.com/andr-andreev/yii2-page-change",
  "type": "project",
  "license": "GPL-3.0",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.0",
    "guzzlehttp/guzzle": "~6.0",
    "league/html-to-markdown": "^4.4",
    "luyadev/luya-bootstrap4": "~1.0.0",
    "sebastian/diff": "~1.4",
    "symfony/dom-crawler": "~3.1",
    "yiisoft/yii2": "^2.0",
    "zelenin/yii2-rss": "~0.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*",
    "squizlabs/php_codesniffer": "3.*"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "config": {
    "sort-packages" : true,
    "process-timeout": 1800
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ],
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  }
}
